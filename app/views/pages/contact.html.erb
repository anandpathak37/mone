<section id="contact_content">
  <div class="element_img hang"><%= image_tag("element_ico9.png", alt: "") %></div>
  <div class="container">
    <h2 class="mheading text-center">Contact Us </h2>
    <div class="row">
      <div class="col-sm-4">
        <div class="contact_content_bx">
          <div class="contact_img">
            <div class="contact_img_inner">
              <div class="contact_ico contact_ico1"></div>
            </div>
          </div>
          <h5>Email</h5>
          <h6><a href="mailto:pri@primitivmedia.com"><%= @connect.email %></a></h6>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="contact_content_bx contact_content_bx2">
          <div class="contact_img">
            <div class="contact_img_inner">
              <div class="contact_ico contact_ico2"></div>
            </div>
          </div>
          <h5>Location</h5>
          <h6><%= @connect.location %><br></h6>
        </div>
      </div>
      <div class="col-sm-4">
        <div class="contact_content_bx contact_content_bx3">
          <div class="contact_img">
            <div class="contact_img_inner">
              <div class="contact_ico contact_ico3"></div>
            </div>
          </div>
          <h5>Phone</h5>
          <h6><%= @connect.phone %></a></h6>
        </div>
      </div>
    </div>
  </div>
</section>
<section id="contact_form_sec">
  <div class="container">
    <div class="contact_frmdiv">
      <div class="element_img hang"><%= image_tag("element_7.png", alt: "") %></div>
      <div class="element_img2 hang"><%= image_tag("element_8.png", alt: "") %></div>
      <form>
        <div class="row">
          <div class="col-sm-6">
            <div class="form-group f-style">
              <label for="exampleInputtext">Your Name</label>
              <input type="text" class="form-control" id="exampleInputtext" >
            </div>
          </div>
          <div class="col-sm-6">
            <div class="form-group f-style">
              <label for="exampleInputemail">Your Email</label>
              <input type="email" class="form-control" id="exampleInputemail" >
            </div>
          </div>
          <div class="col-sm-6">
            <div class="form-group f-style">
              <label for="exampleInputphone">Your Phone</label>
              <input type="text" class="form-control" id="exampleInputphone" >
            </div>
          </div>
          <div class="col-sm-6">
            <div class="form-group f-style">
              <label for="exampleInputsubject">Your Subject</label>
              <input type="text" class="form-control" id="exampleInputsubject" >
            </div>
          </div>
          <div class="col-sm-12">
            <div class="form-group f-style">
              <label for="exampleInputmsg">Your Message</label>
              <textarea class="form-control" id="exampleInputmsg" ></textarea>
            </div>
          </div>
          <div class="text-center col-sm-12">
            <input type="submit" value="Send Messages" class="a_btn">
          </div>
        </div>
      </form>
    </div>
  </div>
</section>
